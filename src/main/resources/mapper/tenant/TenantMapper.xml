         AND TENANT_NAME LIKE '%${tenantName}%'
    </if>
    <if test="state!=null and state != ''">
         AND STATE LIKE '%${state}%'
    </if>
  </select>
  <select id="selectByPrimaryKey" parameterType="java.lang.String" resultMap="BaseResultMap">
