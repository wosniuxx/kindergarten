<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bonc.frame.web.mapper.env.EnvMapper">
	<resultMap type="com.bonc.sso.model.Env" id="BaseResultMap">
		<id column="id" jdbcType="VARCHAR" property="id" />
		<result column="envname" jdbcType="VARCHAR" property="envname" />
		<result column="service" jdbcType="VARCHAR" property="service" />
	</resultMap>
	
	<sql id="Base_Column_List">
	id, envname, service
	</sql>
	
	<select id="selectAll" parameterType="map" resultMap="BaseResultMap">
	    select 
	    <include refid="Base_Column_List" />
	    from SSO_ENV WHERE 1=1
  	</select>
  	
	<select id="selectByPrimaryKey" parameterType="java.lang.String" resultMap="BaseResultMap">
	  select 
	  <include refid="Base_Column_List" />
	  from SSO_ENV
	  where id = #{id,jdbcType=VARCHAR}
	</select>
	
	<select id="selectByEnvName" parameterType="java.lang.String" resultMap="BaseResultMap">
	  select 
	  <include refid="Base_Column_List" />
	  from SSO_ENV
	  where envname = #{envname,jdbcType=VARCHAR}
	</select>
	
	<select id="findAll" resultMap="BaseResultMap">
	    select 
	    <include refid="Base_Column_List" />
	    from SSO_ENV WHERE 1=1
  	</select>
	
</mapper>