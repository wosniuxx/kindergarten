<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bonc.frame.web.mapper.urlExplain.UrlExplainMapper">
	<resultMap type="com.bonc.sso.model.UrlExplain" id="BaseResultMap">
		<id column="id" jdbcType="VARCHAR" property="id" />
		<result column="introduce" jdbcType="VARCHAR" property="introduce" />
		<result column="envname" jdbcType="VARCHAR" property="envname" />
		<result column="getUserUrl" jdbcType="VARCHAR" property="getUserUrl" />
		<result column="targetUrl" jdbcType="VARCHAR" property="targetUrl" />
		<result column="sign" jdbcType="VARCHAR" property="sign" />
		<result column="status" jdbcType="VARCHAR" property="status" />
	</resultMap>
	
	<sql id="Base_Column_List">
	id, introduce, envname, getuserurl, targeturl, sign, status
	</sql>
	
	<select id="selectAll" parameterType="map" resultMap="BaseResultMap">
	    select 
	    <include refid="Base_Column_List" />
	    from SSO_URL WHERE 1=1
  	</select>
  	
	<select id="selectByPrimaryKey" parameterType="java.lang.String" resultMap="BaseResultMap">
	  select 
	  <include refid="Base_Column_List" />
	  from SSO_URL
	  where id = #{id,jdbcType=VARCHAR}
	</select>
	
	<select id="findAll" resultMap="BaseResultMap">
	    select 
	    <include refid="Base_Column_List" />
	    from SSO_URL WHERE 1=1
  	</select>
   
</mapper>