<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bonc.frame.web.mapper.getUserUrl.GetUserUrlMapper">
	<resultMap type="com.bonc.sso.model.GetUserUrl" id="BaseResultMap">
		<id column="id" jdbcType="VARCHAR" property="id" />
		<result column="name" jdbcType="VARCHAR" property="name" />
		<result column="getuserurl" jdbcType="VARCHAR" property="getuserurl" />
		<result column="method" jdbcType="VARCHAR" property="method" />
	</resultMap>
	
	<sql id="Base_Column_List">
	id, name, getuserurl,method
	</sql>
	
	<select id="selectAll" parameterType="map" resultMap="BaseResultMap">
	    select 
	    <include refid="Base_Column_List" />
	    from SSO_GETUSERURL WHERE 1=1
  	</select>
  	
	<select id="selectByPrimaryKey" parameterType="java.lang.String" resultMap="BaseResultMap">
	  select 
	  <include refid="Base_Column_List" />
	  from SSO_GETUSERURL
	  where id = #{id,jdbcType=VARCHAR}
	</select>
	
	<select id="findAll" resultMap="BaseResultMap">
	    select 
	    <include refid="Base_Column_List" />
	    from SSO_GETUSERURL WHERE 1=1
  	</select>
   
</mapper>